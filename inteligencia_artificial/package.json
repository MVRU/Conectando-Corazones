{
  "name": "project-ai-integration",
  "version": "1.0.0",
  "type": "module",
  "description": "Servicio de integraciÃ³n IA para procesar project.completed y generar reportes",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"ts-node --esm\" src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "prompt:luz": "node --loader ts-node/esm src/runPrompt.ts luz_para_aprender",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "test": "npm run build && node --experimental-specifier-resolution=node dist/__tests__/final-report.test.js",
    "test:watch": "npm run test",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "prisma": "^6.16.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.2",
    "@types/supertest": "^6.0.3",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}

